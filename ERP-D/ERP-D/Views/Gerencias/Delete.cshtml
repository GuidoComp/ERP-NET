@model ERP_D.Models.Gerencia

@{
    ViewData["Title"] = "Delete";
}
<div class="form">
<br />
<h3>¿Está seguro de borrar @Model.Nombre?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EsGerenciaGeneral)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EsGerenciaGeneral)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Direccion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Direccion.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Responsable)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Responsable.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Empresa)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Empresa.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CentroDeCosto)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CentroDeCosto.Nombre)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Aceptar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
</div>
